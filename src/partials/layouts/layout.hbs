<!DOCTYPE html>
<html lang="ru">

    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, viewport-fit=cover">
        <meta http-equiv="X-UA-Compatible" content="ie=edge" />
        <meta name="format-detection" content="telephone=no">
        <link rel="stylesheet" href="css/styles.css" />
        <title>
            {{ title }}
        </title>
    </head>

    <body class="no-touch {{#if innerPage}} inner-page {{/if}}" ontouchstart="" >
        <div class="sprite" aria-hidden="true" style="position: absolute; width: 0; height: 0; overflow: hidden;">
            {{> sprite }}
        </div>

        <div class="page-content">
            {{> header }}
            {{> menu }}
            <main class="page-main ">
                {{#block "main"}}

                {{/block}}
            </main>
            {{> footer }}
            {{> newsletter }}
            {{> modals }}
            {{> ie-placeholder }}
            <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
            <script src="https://api-maps.yandex.ru/2.1/?apikey=86fba1b3-37fa-493b-9df4-601bc6d4c97c&lang=ru_RU" type="text/javascript"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.5/gsap.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/ScrollTrigger.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/11.0.5/swiper-bundle.min.js"></script>
            {{> otherScript }}
            <script src="js/app.js"></script>
            {{!-- {{> script }} --}}
            {{#if viewCode}} 
                <script>
                    const submit = $('.js-submit')
                    var pinlogin = $(".js-otp-groups").pinlogin({
                        fields: 4,
                        hideinput: false,
                        reset: false,
                        complete : function(pin){
                            submit.removeAttr('disabled')
                        },
                        keydown : function(e, field, nr){
                            if(field.value.length > 0) field.value = field.value.slice(0, 0);
                            if ((e.keyCode == 37 || e.keyCode == 8) && nr > 0) {
                                submit.prop('disabled', true);
                            }
                        }
                    });
                </script>
            {{/if}}
            {{#if circlePage}}
                <script src="https://cdn.jsdelivr.net/npm/dayjs@1/dayjs.min.js"></script>
                <script src="https://cdnjs.cloudflare.com/ajax/libs/dayjs/1.11.10/plugin/pluralGetSet.min.js" integrity="sha512-7pR4ECKRMtWMJTaHHMUNe6xEYDcOZ7GHjiYFfWjUdk+jcvoAuqhNQHsVJiUnJRrCgfNbDt/ddFIuDtoGhTLK+A==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
                <script src="./assets/js/circle/circle.js"></script>
            {{/if}}
            <script src="assets/js/backend.js"></script>
        </div>
    </body>

</html>
