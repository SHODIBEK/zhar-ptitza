<section class="room js-room-slider">
    <div class="container">
        <div class="room__top-row">
            <div class="room__breadcrumbs">
                <a href="#">
                    Главная
                </a>
                <a href="#">
                    Банные номера
                </a>
                <a href="#">
                    Шале
                </a>
            </div>

            <div class="room__slider-arrows">
                <button class="room__slider-arrow room__slider-arrow--prev">
                    <svg width="14" height="14" aria-hidden="true" class="icon-arrow-left">
                        <use xlink:href="#arrow-left"></use>
                    </svg>
                </button>
                <button class="room__slider-arrow room__slider-arrow--next">
                    <svg width="14" height="14" aria-hidden="true" class="icon-arrow-right">
                        <use xlink:href="#arrow-right"></use>
                    </svg>
                </button>
            </div>
        </div>
        <div class="room__slider">
            <div class="swiper">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <div class="room__slider-card">
                            <div class="room__slider-card-parallax js-parallax-image-wrapper">
                                <img src="img/room/1.jpg" alt="" class="room__slider-card-image">
                            </div>
                        </div>
                    </div>
                    <div class="swiper-slide">
                        <div class="room__slider-card">
                            <div class="room__slider-card-parallax js-parallax-image-wrapper">
